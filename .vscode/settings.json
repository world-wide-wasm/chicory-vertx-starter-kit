{
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "GitHub Light Monochrome Focused",
  "terminal.integrated.fontSize": 16,
  "editor.fontSize": 18,
  "editor.renderLineHighlight": "all",
  "workbench.colorCustomizations": {
    "[GitHub Light Monochrome Focused]": {
      "editor.lineHighlightBackground": "#c6d5e4"
    }
  },
  "go.useLanguageServer": true,
  "go.toolsGopath": "/home/ubuntu/go/",
  "go.gopath": "/home/ubuntu/go/",
  "go.toolsEnvVars": {
    "GOOS": "linux",
    "GOARCH": "arm64",
  },
  "java.configuration.updateBuildConfiguration": "automatic",
  "keyboard.layout": "com.apple.keylayout.French",
}





